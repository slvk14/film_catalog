.card-content
  .form-group
    = simple_form_for [@user] do |f|
      = f.input :first_name, input_html: { maxlength: 50 }  
      = f.input :last_name, input_html: { maxlength: 50 }
      = f.file_field :attachments, multiple: true, direct_upload: true    
      = f.button :submit, "Submit", class: "btn btn-primary waves-effect waves-light"       